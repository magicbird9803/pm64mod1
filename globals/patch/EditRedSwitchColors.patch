#export:Function $FindRSwitchPalettePointer
{
	PUSH	S0, S1, S2, S3
	LAB		S0, 800740AB	%Area byte
	%Check against every area
	LI		S1, 1D
	BEQ		S0, S1, .black
	LI		S1, 1E
	BEQ		S0, S1, .black
	NOP
	.rainbow
	LI		S3, $IntTable_DefaultRSwitchColors
	BEQ		R0, R0, .end
	NOP
	.black
	LI		S3, $IntTable_RBSwitchColors
	BEQ		R0, R0, .end
	NOP
	.end
	SAW		S3, $RSwitchPalettePointer
	POP		S0, S1, S2, S3
	JR		RA
	NOP
}

