{
	PUSH	RA, S0, S1, S2
	LAW		S0, $RSwitchLocation		%<-- will cause problems if invalid pointer			%The stuff around here doesn't seem to affect anything
	LUI		S1, 8000
	AND		S1, S0, S1
	BEQ		S1, R0, .end
	NOP
	ADDI	S0, S0, 28C
	LW		S1, 0 (S0)
	LW		S2, 0 (A0)
	BEQ		S1, S2, .end
	NOP
	.looptop
	LW		S1, 0 (A0)
	BEQ		S1, R0, .end
	NOP	
	SW		S1, 0 (S0)
	ADDI	S0, S0, 10
	ADDI	A0, A0, 4
	BEQ		R0, R0, .looptop
	NOP
	.end
	JPOP	RA, S0, S1, S2
}