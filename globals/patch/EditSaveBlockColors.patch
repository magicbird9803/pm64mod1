%Check if DMA target is a certain value
%Set flag for use in second hook
%Store location
%Execute special functions to modify

#new:Function $DMACopyHook
{
	PUSH	S0, S1, S2, A0, A1, A2, RA
	LI		S0,	E4B2E0
	LI		S1, $SaveBlockFlag
	LI		S2, 1
	BNE		S0, A0, .notSaveBlock
	SW		R0, 0 (S1)
	SW		S2, 0 (S1)
	SAW		A2, $SaveBlockLocation
	JAL		$FindSaveBlockPalettePointer
	NOP
	LAW		A0, $SaveBlockPalettePointer
	JAL		$SetSaveBlockColorWithTable			%Hope this doesn't break stuff!
	NOP
	.notSaveBlock
	LI		S0,	E5AB50
	LI		S1, $LockFlag
	LI		S2, 1
	BNE		S0, A0, .notLock
	SW		R0, 0 (S1)
	SW		S2, 0 (S1)
	SAW		A2, $LockLocation
	JAL		$FindLockPalettePointer
	NOP
	LAW		A0, $LockPalettePointer
	JAL		$SetLockColorWithTable			%Hope this doesn't break stuff!
	NOP
	.notLock
	LI		S0,	E639C0
	LI		S1, $BigSwitchFlag
	LI		S2, 1
	BNE		S0, A0, .notBigSwitch
