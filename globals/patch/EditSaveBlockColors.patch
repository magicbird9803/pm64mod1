#export:Function $WriteIntoDMACopy
{
	%Write into the dma copy function
	LI		V0, $DMACopyHook
	LUI		V1, 00FF
	ORI		V1, V1, FFFF
	AND		V0, V0, V1
	SRL		V0, V0, 2
	LUI		V1, 0800
	ADD		V0, V0, V1
	%Now write into the function
	LI		T0, 80029740
	SW		V0, 0 (T0)
	ADDI	T0, T0, 4
	SW		R0, 0 (T0)
	%Second hook
	%Write into the dma copy function
	LI		V0, $DMACopyHook2
	LUI		V1, 00FF
	ORI		V1, V1, FFFF
	AND		V0, V0, V1
	SRL		V0, V0, 2
	LUI		V1, 0800
	ADD		V0, V0, V1
	%Now write into the function
	LI		T0, 800297B8
	SW		V0, 0 (T0)
	ADDI	T0, T0, 4
	SW		R0, 0 (T0)
	%Delete this function call (second very hacky thing)
	DADDU	V0, RA, R0
	ADDI	V0, V0, FFF8
	SW		R0, 0 (V0)
	%
	JR		RA
	NOP
}

