#export:Function $FindBigSwitchPalettePointer
{
	PUSH	S0, S1, S2, S3
	LAB		S0, 800740AB	%Area byte
	%Check against every area
	LI		S1, 1C
	BEQ		S0, S1, .black
	LI		S1, 1D
	BEQ		S0, S1, .black
	LI		S1, 1E
	BEQ		S0, S1, .black
	NOP
	.rainbow
	LI		S3, $IntTable_DefaultBigSwitchColors
	BEQ		R0, R0, .end
	NOP
	.black
	LI		S3, $IntTable_DarkBigSwitchColors
	BEQ		R0, R0, .end
	NOP
	.end
	SAW		S3, $BigSwitchPalettePointer
	POP		S0, S1, S2, S3
	JR		RA
	NOP
}

%function that will work with the above data structures
%Will not copy if the first value is identical for both
%A0 = int table
#export:Function $SetBigSwitchColorWithTable
{
	PUSH	RA, S0, S1, S2
	LAW		S0, $BigSwitchLocation		%<-- will cause problems if invalid pointer			%The stuff around here doesn't seem to affect anything
	LUI		S1, 8000
	AND		S1, S0, S1
	BEQ		S1, R0, .end
	NOP
	ADDI	S0, S0, 30C
	LW		S1, 0 (S0)
	LW		S2, 0 (A0)
	BEQ		S1, S2, .end
	NOP
	.looptop
	LW		S1, 0 (A0)
	BEQ		S1, R0, .end
	NOP	
	SW		S1, 0 (S0)
	ADDI	S0, S0, 10
	ADDI	A0, A0, 4
	BEQ		R0, R0, .looptop
	NOP
	.end
	JPOP	RA, S0, S1, S2
}