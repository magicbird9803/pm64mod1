#new:Script $Script_Init_BossFight
{
	0:  Call     BindNpcIdle 	( .Npc:Self $Script_BossFight )
		Call     BindNpcDefeat  ( .Npc:Self $Script_Defeat )
   14:  Call     SetNpcFlagBits ( .Npc:Self 00000100 .True )
   2C:  Call     SetNpcFlagBits ( .Npc:Self 00000200 .False )
   44:  Return
   4C:  End
}

#new:Script $Script_Defeat %get rid of everyone
{
	Call GetBattleOutcome (*Var[0])
	Switch *Var[0]
		Case == 00000000
			Set *VarF 2D
			Loop 10`
				Call	RemoveNpc   	( *Var[F] )
				Add *VarF 1
			EndLoop
	EndSwitch
	Return
	End
}

#new:Script $Script_BossFight %hope this works
{
Loop
	If *AreaByte[F] == 1
		Call     StartBossBattle 	( .Song:FinalBowserBattle ) %;)
	EndIf
	Wait	00000001
EndLoop
Return
End
}


